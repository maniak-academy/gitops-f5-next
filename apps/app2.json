{
  "class": "ADC",
  "schemaVersion": "3.0.0",
  "Backend": {
    "class": "Tenant",
    "Backend": {
      "class": "Application",
      "template": "http",
      "serviceMain": {
        "pool": "Backend-Pool",
        "snat": "auto",
        "virtualPort": 80,
        "virtualAddresses": ["172.16.40.81"],
        "class": "Service_HTTP"
      },
      "Backend-Pool": {
        "members": [{
          "servicePort": 80,
          "serverAddresses": ["172.16.40.22","172.16.40.30"]
        }],
        "loadBalancingMode": "round-robin",
        "class": "Pool"
      }
    }
  }
}