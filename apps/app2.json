{
  "class": "ADC",
  "schemaVersion": "3.12.0",
  "WebApp2": {
    "class": "Tenant",
    "WebApp2": {
      "class": "Application",
      "template": "http",
      "serviceMain": {
        "pool": "WebApp2-Pool",
        "snat": "auto",
        "virtualPort": 80,
        "virtualAddresses": ["172.16.40.81"],
        "class": "Service_HTTP"
      },
      "WebApp2-Pool": {
        "members": [{
          "servicePort": 80,
          "sharedNodes": true,
          "serverAddresses": ["172.16.40.22","172.16.40.30"]
        }],
        "loadBalancingMode": "round-robin",
        "class": "WebApp2-Pool"
      }
    }
  }
}