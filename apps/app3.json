{
  "class": "ADC",
  "schemaVersion": "3.0.0",
  "WebFront": {
    "class": "Tenant",
    "WebApp2": {
      "class": "Application",
      "template": "http",
      "serviceMain": {
        "pool": "WebApp2-Pool",
        "snat": "auto",
        "virtualPort": 80,
        "virtualAddresses": ["172.16.40.80"],
        "class": "Service_HTTP"
      },
      "WebApp2-Pool": {
        "members": [{
          "servicePort": 80,
          "serverAddresses": ["172.16.40.35","172.16.40.36"]
        }],
        "loadBalancingMode": "round-robin",
        "class": "Pool"
      }
    }
  }
}