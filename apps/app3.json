{
  "class": "ADC",
  "schemaVersion": "3.0.0",
  "WebApp3": {
    "class": "Tenant",
    "WebApp3": {
      "class": "Application",
      "template": "http",
      "serviceMain": {
        "pool": "WebApp3-Pool",
        "snat": "auto",
        "virtualPort": 80,
        "virtualAddresses": ["172.16.40.87"],
        "class": "Service_HTTP"
      },
      "WebApp3-Pool": {
        "members": [{
          "servicePort": 80,
          "serverAddresses": ["172.16.40.3","172.16.40.6"]
        }],
        "loadBalancingMode": "round-robin",
        "class": "Pool"
      }
    }
  }
}