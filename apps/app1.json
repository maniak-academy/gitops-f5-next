{
    "class": "ADC",
    "schemaVersion": "3.12.0",
    "WebApp1": {
      "class": "Tenant",
      "WebApp1": {
        "class": "Application",
        "template": "http",
        "serviceMain": {
          "pool": "WebApp1-Pool",
          "snat": "auto",
          "virtualPort": 80,
          "virtualAddresses": ["172.16.40.80"],
          "class": "Service_HTTP"
        },
        "WebApp1-Pool": {
          "members": [{
            "servicePort": 80,
            "sharedNodes": true,
            "serverAddresses": ["172.16.40.20","172.16.40.21"]
          }],
          "loadBalancingMode": "round-robin",
          "class": "WebApp1-Pool"
        }
      }
    }
}